<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/doodlee"

    tools:context=".SignUpActivity">

    <EditText
        android:id="@+id/signupdname"
        android:layout_width="299dp"
        android:layout_height="46dp"
        android:layout_marginTop="208dp"
        android:drawableLeft="@drawable/ic_baseline_person"
        android:ems="10"
        android:hint="   Enter Name"
        android:inputType="textPersonName"
        android:textColorHint="@color/black"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/signupdage"
        android:layout_width="299dp"
        android:layout_height="46dp"
        android:layout_marginTop="20dp"
        android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890_-!@#$%^*()"
        android:drawableLeft="@drawable/ic_baseline_age"
        android:ems="10"
        android:hint="   Enter Age"
        android:inputType="textPersonName"
        android:maxLength="10"
        android:textColorHint="@color/black"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signupdname" />

    <EditText
        android:id="@+id/signupdqualificaton"
        android:layout_width="299dp"
        android:layout_height="54dp"
        android:layout_marginTop="20dp"
        android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890_-!@#$%^*()"
        android:drawableLeft="@drawable/ic_baseline_library_books_24"
        android:ems="10"
        android:hint="   Enter Qualification"
        android:maxLength="10"
        android:textColorHint="@color/black"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signupdage" />

    <EditText
        android:id="@+id/sigupdexp"
        android:layout_width="299dp"
        android:layout_height="54dp"
        android:layout_marginTop="20dp"
        android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890_-!@#$%^*()"
        android:drawableLeft="@drawable/ic_baseline_explicit_24"
        android:ems="10"
        android:hint="   Enter Experience"
        android:maxLength="10"
        android:textColorHint="@color/black"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/signupdqualificaton" />

    <EditText
        android:id="@+id/signupdemail"
        android:layout_width="299dp"
        android:layout_height="54dp"
        android:layout_marginTop="20dp"
        android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890._-!@#$%^*()"
        android:drawableLeft="@drawable/ic_baseline_email_24"
        android:ems="10"
        android:hint="   Enter Email"
        android:textColorHint="@color/black"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sigupdexp" />

    <EditText
        android:id="@+id/signupdpssd"
        android:layout_width="299dp"
        android:layout_height="54dp"
        android:layout_marginTop="20dp"
        android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890_-!@#$%^*()"
        android:drawableLeft="@drawable/ic_baseline_security_24"
        android:ems="10"
        android:hint="   Enter Password"
        android:inputType="textPassword"
        android:maxLength="10"
        android:textColorHint="@color/black"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signupdemail" />

    <Button
        android:id="@+id/dsignupbutton"
        android:layout_width="255dp"
        android:layout_height="64dp"
        android:text="Sign Up"
        android:textAllCaps="false"
        android:textColor="#fff"
        android:background="@drawable/input_bg"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signupdpssd"
        app:layout_constraintVertical_bias="0.368" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="120dp"
        android:layout_height="42dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="Doctor"
        android:textColorHint="@color/black"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/signupdname"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.018"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="172dp"
        android:layout_height="144dp"
        app:layout_constraintBottom_toTopOf="@+id/signupdname"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/doctorlogo" />

</androidx.constraintlayout.widget.ConstraintLayout>